import pygame

class Rock(pygame.sprite.Sprite):
    def __init__(self, x, y):
        super().__init__()
        self.image = pygame.Surface((20, 20))
        self.image.fill((128, 128, 128))  # Fill the image with gray color
        
        self.rect = self.image.get_rect()
        self.rect.x = x
        self.rect.y = y
